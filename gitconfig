[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[alias]
	lol = log --graph --decorate --pretty=oneline --abbrev-commit --all
	lg = log --color --pretty=format:'%Cred%h%Creset - %s %Cgreen(%ar)%Creset %an'
	s = status -s
	d = diff -w --patience
  mustash = stash -u
	sweep = diff master --name-status --color -- app/
	pr = pull-request
[color]
	ui = true
[core]
	editor = vim
